import tkinter as tk

root = tk.Tk()

# Create labels with different relief styles
label_flat = tk.Label(root, text="Flat", relief=tk.FLAT)
label_sunken = tk.Label(root, text="Sunken", relief=tk.SUNKEN)
label_raised = tk.Label(root, text="Raised", relief=tk.RAISED)
label_groove = tk.Label(root, text="Groove", relief=tk.GROOVE)
label_ridge = tk.Label(root, text="Ridge", relief=tk.RIDGE)

# Pack labels
label_flat.pack()
label_sunken.pack()
label_raised.pack()
label_groove.pack()
label_ridge.pack()

root.mainloop()